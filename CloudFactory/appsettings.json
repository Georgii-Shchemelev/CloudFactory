{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessageBroker": {
    "Directory": "%programdata%\\MessageBroker",
    "RequestExtension": ".req",
    "ResponseFilter": "*.resp"
  },
  "AllowedHosts": "*",
  "UseAdvancedImplementation" :  true

}
